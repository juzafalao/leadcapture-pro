{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/dashboard/(.*)",
      "dest": "/dashboard-build/$1"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard-build/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
